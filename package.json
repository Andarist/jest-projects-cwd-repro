  },
  "homepage": "https://github.com/Andarist/jest-projects-cwd-repro#readme",
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "jest": "^24.9.0"
  }
}
