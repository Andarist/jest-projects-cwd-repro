{
  "name": "jest-projects-cwd-repro",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "test:packages": "jest --projects ./packages/*",
    "test:dir": "npm run test --prefix packages/test",
    "repro": "npm run test:dir && npm run test:packages"
  },
